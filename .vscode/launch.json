{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/linker",
      "args": ["-hex",
         "-place=ivt@0x0000",
          "-o",
          "/home/ss/Desktop/ss_project/program.hex",
           "/home/ss/Desktop/ss_project/main.o",
          "/home/ss/Desktop/ss_project/isr_reset.o" ,
          "/home/ss/Desktop/ss_project/isr_terminal.o" ,
          "/home/ss/Desktop/ss_project/isr_timer.o ",
          "/home/ss/Desktop/ss_project/ivt.o"],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
          {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
          },
          {
              "description":  "Set Disassembly Flavor to Intel",
              "text": "-gdb-set disassembly-flavor intel",
              "ignoreFailures": true
          }
      ]
    }
  
  ]
}